@prefix ontop: <http://w3id.org/ontop/>.

@prefix rdfp: <http://w3id.org/rdfp/>.
@base <https://w3id.org/rdfp/>.

<> a rdfp:Graph ; ontop:fileName "rdfp-1.0.ttl" ;
  rdfp:alias <index>, <1.0> .

<example/description> a rdfp:Graph ; ontop:filePath "example-description.ttl" .

<example/graph/xml/liftingRule> a ontop:Resource ; ontop:representedBy [ ontop:filePath "graph-xml-liftingRule.rqg" ;  ontop:mediaType "application/vnd.sparql-generate" ] .
<example/graph/xml/loweringRule> a ontop:Resource ; ontop:representedBy [ ontop:filePath "graph-xml-loweringRule.rul" ;  ontop:mediaType "application/vnd.corese-rules" ] .
 
<example/graph/json/liftingRule> a ontop:Resource ; ontop:representedBy [ ontop:filePath "graph-json-liftingRule.rqg" ;  ontop:mediaType "application/vnd.sparql-generate" ] .
<example/graph/json/loweringRule> a ontop:Resource ; ontop:representedBy [ ontop:filePath "graph-json-loweringRule.rul" ;  ontop:mediaType "application/vnd.corese-rules" ] .

<presentation3/liftingRule> a ontop:Resource ; ontop:representedBy [ ontop:filePath "presentation3-liftingRule.rqg" ;  ontop:mediaType "application/vnd.sparql-generate" ] .
<presentation2/loweringRule> a ontop:Resource ; ontop:representedBy [ ontop:filePath "presentation2-loweringRule.rul" ;  ontop:mediaType "application/vnd.corese-rules" ] .


<example/input> a ontop:Resource ; 
  ontop:representedBy [ ontop:filePath "input-example.json" ;  ontop:mediaType "application/json" ] ;
  ontop:representedBy [ ontop:filePath "input-example.ttl" ;  ontop:mediaType "text/turtle" ] ;
  ontop:representedBy [ ontop:filePath "input-example.xml" ;  ontop:mediaType "application/xml" ] ;
  ontop:representedBy [ ontop:filePath "input-example.txt" ;  ontop:mediaType "text/plain" ] .
